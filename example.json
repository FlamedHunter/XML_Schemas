{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RootRecord",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "payload": {
      "type": "object",
      "properties": {
        "root": {
          "type": "object",
          "properties": {
            "item": {
              "type": "string"
            }
          },
          "required": ["item"]
        }
      },
      "required": ["root"]
    }
  },
  "required": ["id", "payload"]
}
